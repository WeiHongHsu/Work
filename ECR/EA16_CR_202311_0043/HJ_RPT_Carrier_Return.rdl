<?xml version="1.0" encoding="utf-8"?>
<Report MustUnderstand="df" xmlns="http://schemas.microsoft.com/sqlserver/reporting/2016/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner" xmlns:df="http://schemas.microsoft.com/sqlserver/reporting/2016/01/reportdefinition/defaultfontfamily">
  <Description>退供-承運商運送報</Description>
  <df:DefaultFontFamily>Segoe UI</df:DefaultFontFamily>
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="HJ_PROD_AAD">
      <DataSourceReference>/HJ_AAD_LINK</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>97c22122-9431-437a-9ac7-b2f4735444d7</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>HJ_PROD_AAD</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@DATE_START">
            <Value>=Parameters!DATE_START.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DATE_END">
            <Value>=Parameters!DATE_END.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@REG_NUM">
            <Value>=Parameters!REG_NUM.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT CONVERT(VARCHAR,CARM.ship_dt,111) AS N'發運日'
,CARM.reg_number AS N'發運批次號'
,CARM.ship_to_code AS N'供應商代號'
,VENT.vendor_name_full AS N'供應商名稱'
,RTRIM(VENT.phone_number) AS '供應商倉庫電話'
,RTRIM(VENT.address)  AS N'倉庫地址'
,case when CARM.master_carton_number like 'WV%' then CARM.carton_label else CARM.master_carton_number end AS N'箱號'  --2024-01-10 Emma 修改
,VENT.esl_po_descr AS N'採購類型'
,VENT.esl_carrier AS N'承運商'
,CARM.total_qty N'數量'
--,CASE WHEN ISNULL(CARM.merge_qty,'')=''
--      THEN CARM.total_qty
--	  ELSE CARM.merge_qty
--	  END N'數量'
,'' AS N'付款方式'
FROM _t_order_carton_master CARM WITH (NOLOCK) 
INNER JOIN _tv_vendor VENT WITH (NOLOCK)
ON CARM.ship_to_code = VENT.display_vendor_code
WHERE ( carton_type In('RSO','REJ')OR LEFT(display_po_number,3) in ('505','515')--Hyper care 增加退供差異箱號邏輯
--LEFT(CARM.display_po_number,3) in ('505','515') 
 OR (carton_type in('ZP') AND LEFT(display_order_number,1) in ('4')))  --2024-01-10 Emma 新增4開頭退供單號
AND CARM.client_code = VENT.client_code
AND CONVERT(VARCHAR,CARM.ship_dt,112) &gt;= @DATE_START
AND CONVERT(VARCHAR,CARM.ship_dt,112) &lt;= @DATE_END
AND CARM.reg_number = IIF(@REG_NUM = '', CARM.reg_number, @REG_NUM)
ORDER BY ship_dt</CommandText>
        <Timeout>15</Timeout>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
